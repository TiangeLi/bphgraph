{
    "version": 2,
    "builds": [
      {
        "src": "langserver.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "langserver.py"
      }
    ],
    "functions": {
      "api/**": {
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    }
}